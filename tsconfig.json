{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "types": ["jest-styled-components"],
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"],
      "tests": ["tests/*"],
      "@components/*": ["src/components/*"],
      "@screens/*": ["src/screens/*"],
      "@utils/*": ["src/utils/*"],
      "@data/*": ["src/data/*"],
      "@navigations/*": ["src/navigations/*"],
      "@theme/*": ["src/theme/*"],
    }
  }
}
